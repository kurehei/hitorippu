<div class='timeline'>
  <% if posts.empty?%>
   
  <% else %>
    <% posts.each do |postings| %>
        <div class="post">
          <div class="col-md-9">
            <div class="panel panel-default">
              <div class="panel-heading text-center">
                <p class='item-title text-left'>
                　　
                　　<%= image_tag avatar_url(current_user) ,class:"img-circle",:size =>'30x30'%>
                  <%=link_to postings.user.name, user_path(current_user)%>
                </p>
                <div class='id'><%= link_to postings.name ,postings%></div>
                
              </div>
              <div class="panel-body">
                      <%=link_to postings do%>
                      　<%= image_tag postings.image.thumb.url ,:size =>'570x335'%>
                      <% end%>
  
                
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  <% end %>
</div>